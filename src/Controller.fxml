<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller">
   <children>
      <SubScene fx:id="subScene1" height="737.0" layoutY="73.0" pickOnBounds="true" width="800.0">
        <root>
          <Region />
        </root>
      </SubScene>
      <GridPane fx:id="mainPane" prefHeight="75.0" prefWidth="800.0" style="-fx-border-width: 1; -fx-border-color: grey; -fx-border-insets: 3; -fx-border-style: none;">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <GridPane fx:id="loadFilePane" hgap="5.0" prefHeight="60.0" prefWidth="275.0" style="-fx-border-width: 1; -fx-border-color: grey; -fx-border-insets: 1; -fx-border-style: none;" vgap="5.0">
              <columnConstraints>
                <ColumnConstraints halignment="RIGHT" hgrow="SOMETIMES" minWidth="0.0" percentWidth="30.0" prefWidth="1.0" />
                <ColumnConstraints halignment="LEFT" hgrow="ALWAYS" minWidth="10.0" percentWidth="60.0" prefWidth="100.0" />
                  <ColumnConstraints halignment="LEFT" hgrow="SOMETIMES" minWidth="10.0" percentWidth="20.0" prefWidth="100.0" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <Button fx:id="saveButton" mnemonicParsing="false" onMouseClicked="#onSaveButtonPressed" text="Save " GridPane.columnIndex="2" GridPane.rowIndex="1" />
                  <ComboBox fx:id="filesComboBox" prefWidth="150.0" GridPane.columnIndex="1" />
                  <Button fx:id="loadButton" mnemonicParsing="false" onMouseClicked="#onLoadFileButtonPressed" text="Load File" GridPane.columnIndex="2" />
                  <Label alignment="CENTER" text="Select File:" />
                  <Label alignment="CENTER" text="Save File:" GridPane.rowIndex="1" />
                  <TextField fx:id="fileTextField" GridPane.columnIndex="1" GridPane.rowIndex="1" />
               </children>
            </GridPane>
            <GridPane fx:id="graphOptionPane" hgap="5.0" style="-fx-border-width: 1; -fx-border-color: grey; -fx-border-insets: 1; -fx-border-style: none;" vgap="5.0" GridPane.columnIndex="1">
              <columnConstraints>
                <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" minWidth="10.0" percentWidth="30.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <GridPane fx:id="graphPane" alignment="CENTER" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.valignment="CENTER">
                    <columnConstraints>
                      <ColumnConstraints halignment="RIGHT" minWidth="10.0" percentWidth="30.0" prefWidth="100.0" />
                      <ColumnConstraints halignment="LEFT" hgrow="ALWAYS" minWidth="10.0" prefWidth="100.0" />
                    </columnConstraints>
                    <rowConstraints>
                      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                    </rowConstraints>
                     <padding>
                        <Insets bottom="5.0" left="5.0" right="5.0" />
                     </padding>
                  </GridPane>
                  <GridPane hgap="1.0" vgap="1.0">
                    <columnConstraints>
                      <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                    </columnConstraints>
                    <rowConstraints>
                      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                    </rowConstraints>
                     <children>
                        <ComboBox fx:id="graphComboBox" onAction="#onGraphComboBoxAction" prefWidth="150.0" GridPane.rowIndex="1" />
                        <Label text="Select Option:" />
                     </children>
                  </GridPane>
               </children>
            </GridPane>
         </children>
      </GridPane>
   </children>
</AnchorPane>
